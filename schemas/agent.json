            },
            "denyByDefault": {
              "anyOf": [
                {
                  "type": "boolean"
                },
                {
                  "type": "null"
                }
              ]
            },
            "forceAllow": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false,
          "title": "ExecuteShellTool"
        },
        "web-fetch": {
          "type": "object",
